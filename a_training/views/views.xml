        <record id="event_event_form" model="ir.ui.view">
            <field name="name">event.event.form</field>
            <field name="model">event.event</field>
            <field name="inherit_id" ref="event.view_event_form" />
            <field name="arch" type="xml">
                <xpath expr="//button[@name='button_confirm']" position="attributes">
                    <attribute name="groups">a_role.group_station_manager</attribute>
                </xpath>
                <xpath expr="//button[@name='button_confirm']" position="attributes">
                    <attribute name="groups">a_role.group_section_head</attribute>
                </xpath>
                <xpath expr="//field[@name='organizer_id']" position="before">
                    <field name="training_id"/>
                </xpath>
                <xpath expr="//field[@name='user_id']" position="after">
                    <field name="trainer_id"/>
                </xpath>
                <xpath expr="//field[@name='event_type_id']" position="before">
                    <field name="pre_test_id"/>
                    <field name="post_test_id"/>
                </xpath>
            </field>
        </record>

        <record id="event_registrasion_form" model="ir.ui.view">
            <field name="name">event.registration.form</field>
            <field name="model">event.registration</field>
            <field name="inherit_id" ref="event.view_event_registration_form" />
            <field name="arch" type="xml">
                <xpath expr="//sheet/group" position="before">
                    <div class="oe_button_box" name="button_box" groups="base.group_user">
                        <button name="button_test" type="object" class="oe_stat_button" icon="fa-users">
                            <field name="pre_test_ids" widget="statinfo" string="Pre-Test Form"/>
                        </button>
                        <button name="button_test" type="object" class="oe_stat_button" icon="fa-users">
                            <field name="post_test_ids" widget="statinfo" string="Post-Test Form"/>
                        </button>
                    </div>
                </xpath>
                <xpath expr="//field[@name='partner_id']" position="before">
                    <field name="employee_id"/>
                </xpath>
            </field>
        </record>


